{
  "name": "chronos",
  "version": "2.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:proxy": "ng serve --proxy-config proxy.conf.js",
    "build": "ng build",
    "build:prod": "ng build chronos --configuration production",
    "test": "ng test",
    "test:quiet": "ng test -c quiet",
    "lint": "ng lint",
    "lint:staged": "lint-staged --relative",
    "generate-icons": "svg-generator",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.3",
    "@angular/cdk": "~13.1.3",
    "@angular/common": "~13.1.3",
    "@angular/compiler": "~13.1.3",
    "@angular/core": "~13.1.3",
    "@angular/forms": "~13.1.3",
    "@angular/material": "~13.1.3",
    "@angular/platform-browser": "~13.1.3",
    "@angular/platform-browser-dynamic": "~13.1.3",
    "@angular/router": "~13.1.3",
    "@ngneat/svg-icon": "^4.0.2",
    "@ngneat/until-destroy": "^9.0.0",
    "decentr-js": "^4.2.2",
    "highcharts": "^9.3.2",
    "highcharts-angular": "^3.0.0",
    "ngx-google-analytics": "^11.0.0",
    "ngx-json-viewer": "^3.0.2",
    "ngx-scrollbar": "^9.0.1",
    "ngx-skeleton-loader": "^4.0.0",
    "rxjs": "~7.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular-eslint/builder": "^13.1.0",
    "@angular-eslint/eslint-plugin": "^13.1.0",
    "@angular-eslint/eslint-plugin-template": "^13.1.0",
    "@angular-eslint/schematics": "^13.1.0",
    "@angular-eslint/template-parser": "^13.1.0",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.3",
    "@angular/language-service": "~13.1.3",
    "@ngneat/svg-generator": "^3.2.1",
    "@types/jasmine": "~3.10.3",
    "@types/node": "^12.20.42",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.12",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lint-staged": "^12.3.7",
    "typescript": "~4.5.5"
  },
  "svgGenerator": {
    "outputPath": "./src/app/_shared/svg-icons",
    "prefix": "svg",
    "srcPath": "./src/assets/svg",
    "svgoConfig": {
      "plugins": [
        "cleanupAttrs",
        "removeDimensions"
      ]
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint"
    ]
  }
}
